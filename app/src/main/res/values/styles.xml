<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- Tema proprio -->
    <style name="TemaProprio" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/laranja</item>
        <item name="colorPrimaryDark">@color/laranja_claro</item>
        <item name="colorAccent">@color/branco</item>
        <item name="android:textColor">@color/branco</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:windowBackground">@color/cinza</item>
    </style>

</resources>
